{
  "name": "backend_boilerplate",
  "version": "0.0.1",
  "description": "basic blank project setup. Hopefully saves you time configuring",
  "main": "index.js",
  "scripts": {
    "swagger": "tsoa swagger && tsoa routes",
    "build": "npm run swagger && npm run lint",
    "lint": "tslint --force 'src/**/*.ts' > ./tslint.err",
    "start": "npm run build && nodemon",
    "dockerstart": "npm install && npm run start",
    "test": "ls && set NODE_ENV=TEST && nyc --extension .ts --extension .tsx mocha --timeout 10000 -r ts-node/register ./test/*.ts && nyc report --reporter text-lcov --extension .ts --extension .tsx | coveralls"
  },
  "engines": {
    "node": "8.11.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KyleGalvin/backendBoilerplate.git"
  },
  "author": "Kyle Galvin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KyleGalvin/backendBoilerplate/issues"
  },
  "homepage": "https://github.com/KyleGalvin/backendBoilerplate#readme",
  "devDependencies": {
    "coveralls": "^3.0.0",
    "mocha": "^5.0.5",
    "mocha-lcov-reporter": "^1.3.0",
    "mocha-typescript": "1.1.12",
    "nodemon": "^1.12.1",
    "nyc": "^13.0.1",
    "source-map-support": "^0.5.0"
  },
  "dependencies": {
    "@types/bcrypt": "^2.0.0",
    "@types/body-parser": "^1.16.5",
    "@types/bunyan": "^1.8.5",
    "@types/cors": "^2.8.1",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/multer": "^1.3.6",
    "@types/node": "^10.11.2",
    "@types/sql.js": "0.0.29",
    "@types/uuid": "^3.4.3",
    "bcrypt": "^3.0.1",
    "body-parser": "^1.18.2",
    "bunyan": "^1.8.12",
    "bunyan-lumberjack": "^1.1.3",
    "cors": "^2.8.4",
    "editorconfig": "0.15.0",
    "express": "^4.16.3",
    "https": "^1.0.0",
    "jsonwebtoken": "^8.1.0",
    "multer": "^1.3.0",
    "npm": "^6.4.1",
    "pg": "^7.3.0",
    "reflect-metadata": "^0.1.10",
    "sql.js": "^0.5.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tsoa": "^2.2.5",
    "typemoq": "^2.1.0",
    "typeorm": "^0.2.0",
    "typescript": "^3.1.1",
    "typescript-ioc": "^1.1.0"
  }
}
